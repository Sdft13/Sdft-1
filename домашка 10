import sqlite3 as sql
connection = sql.connect('bank.sl3', 5)
cur = connection.cursor()
cur.execute("create table bank (id INT, first_name TEXT, last_name TEXT, balance INT)")
cur.execute("INSERT INTO bank VALUES (1, 'John', 'krum', 20)")
cur.execute("INSERT INTO bank VALUES (2, 'Bo', 'sinn', 40)")
cur.execute("INSERT INTO bank VALUES (3, 'ARtem', 'lebedev', 1)")
cur.execute("INSERT INTO bank VALUES (4, 'Talier', 'Dernul', 90)")
cur.execute("INSERT INTO bank VALUES (5, 'vicktor', 'choi', 70)")
cur.execute('Select * from bank')
res = cur.fetchall()
print(res)
connection.commit()
connection.close()
